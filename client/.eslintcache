           {/* onChange 는 option 에서 내가 고른 value 값을 setCategory 에 지정하여 category 값을 업데이트한다 */}\n            <select\n              name='category'\n              className='form-control'\n              onChange={(e) => setParent(e.target.value)}\n            >\n              <option>Please Select</option>\n              {categories.length > 0 &&\n                categories.map((c) => (\n                  <option key={c._id} value={c._id} selected={c._id === parent}>\n                    {c.name}\n                  </option>\n                ))}\n            </select>\n          </div>\n\n          {/* 스크린에 category 값이 무엇으로 업데이트 되는지 확인하기 위해 잠시 쓰는 방법 */}\n          {/* {JSON.stringify(category)} */}\n\n          <CategoryForm\n            name={name}\n            setName={setName}\n            handleSubmit={handleSubmit}\n          />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default SubUpdate;\n","/Users/joon/Downloads/ecommerce/e-clone/client/src/pages/admin/product/ProductCreate.js",["137","138","139","140","141","142","143","144"],"/Users/joon/Downloads/ecommerce/e-clone/client/src/functions/product.js",[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":19,"column":6,"nodeType":"151","endLine":19,"endColumn":8,"suggestions":"152"},{"ruleId":"149","severity":1,"message":"153","line":19,"column":6,"nodeType":"151","endLine":19,"endColumn":8,"suggestions":"154"},{"ruleId":"155","severity":1,"message":"156","line":1,"column":27,"nodeType":"157","messageId":"158","endLine":1,"endColumn":36},{"ruleId":"155","severity":1,"message":"159","line":33,"column":5,"nodeType":"157","messageId":"158","endLine":33,"endColumn":15},{"ruleId":"155","severity":1,"message":"160","line":34,"column":5,"nodeType":"157","messageId":"158","endLine":34,"endColumn":13},{"ruleId":"155","severity":1,"message":"161","line":35,"column":5,"nodeType":"157","messageId":"158","endLine":35,"endColumn":9},{"ru